{
	"build": {
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build",
		"devPath": "http://localhost:1420",
		"distDir": "../dist",
		"withGlobalTauri": false
	},
	"package": {
		"productName": "knopka",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": true,
			"shell": {
				"all": false,
				"open": true
			}
		},
		"bundle": {
			"active": true,
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "knopka",
			"targets": "all"
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "knopka",
				"width": 800,
				"height": 600,
				"alwaysOnTop": true
			}
		]
	}
}
